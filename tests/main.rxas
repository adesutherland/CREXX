.globals=0

* g0 .expose=my.global

main()         .locals=3
 load r1,"Hello"
 load r2,"Rene"
 sconcat r2,r1,r2
 ssay r2
 say "\n"

               load r0,"test - main() - calling func1()"
               ssay r0
               say "\n"
               call func1()
               load r0,"test - main() - calling func2()"
               ssay r0
               say "\n"
               call func2()
               ret

log()          .locals=1 .expose=my.log
               load r0,"Log: "
               ssay r0
               ssay a1
               say "\n"
               ret

func1()       .expose=global.func1

func2()       .expose=global.func2
