/* reverse */
          .globals=0
reverse() .locals=4 .expose=global.reverse
   copy   r1,a1         /* string to reverse  */
   strlen r2,r1        /* length             */
   dec    r2
   load   R3,""         /* reversed string    */
loop:
    concchar r3,r1,r2
    bct loop,r2        /* continue to look for next non blank char */
    concchar r3,r1,r2 /* byte 0 needs to be concattenated too      */
    say r3
    ret r3